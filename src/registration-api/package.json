{
  "name": "registration-api",
  "version": "1.0.0",
  "private": true,
  "main": "./dist/src/functions/**/*.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "clean": "rimraf dist",
    "prestart": "npm run clean",
    "dev:storage": "azurite --silent --location ./.azurite --debug ./.azurite/debug.log",
    "start:host": "func start --port 7072",
    "start": "concurrently npm:start:* npm:watch --raw --kill-others",
    "update:local-settings": "node ./scripts/update-local-settings.mjs",
    "postinstall": "npm run update:local-settings"
  },
  "author": "Microsoft",
  "license": "MIT",
  "template": "functions",
  "dependencies": {
    "@azure/cosmos": "^4.3.0",
    "@azure/functions": "^4",
    "@azure/identity": "^4.9.1",
    "dotenv": "^16"
  },
  "devDependencies": {
    "@types/node": "^20",
    "azure-functions-core-tools": "^4",
    "azurite": "^3",
    "concurrently": "^9",
    "rimraf": "^6.0.1",
    "typescript": "^5"
  }
}
